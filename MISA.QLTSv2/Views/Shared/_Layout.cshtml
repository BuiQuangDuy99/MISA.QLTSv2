<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["ChildTitle"] - MISA.QLTSv2</title>
    <link href="~/lib/jquery-ui-1.12.1.custom/jquery-ui.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/uicons-solid-rounded/css/uicons-solid-rounded.css" rel="stylesheet" />
    <link href="~/css/common/common.css" rel="stylesheet" />
    <link href="~/css/common/main.css" rel="stylesheet" />
    <link href="~/css/common/navbar.css" rel="stylesheet" />
    <link href="~/css/common/header.css" rel="stylesheet" />
    <link href="~/css/common/content.css" rel="stylesheet" />
    <link href="~/css/common/button.css" rel="stylesheet" />
    <link href="~/css/common/grid.css" rel="stylesheet" />
    <link href="~/css/common/icon.css" rel="stylesheet" />
    <link href="~/css/common/input.css" rel="stylesheet" />
    <link href="~/css/common/layout.css" rel="stylesheet" />
    <link href="~/css/common/toolbar.css" rel="stylesheet" />
    <link href="~/css/common/loading.css" rel="stylesheet" />
    <link href="~/css/common/responsive.css" rel="stylesheet" />
    <link href="~/css/custom/department.css" rel="stylesheet" />
    @*<link href="~/css/Form/depreciation.css" rel="stylesheet" />*@
    <link href="~/css/Form/dictionaryForm.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/css/asset.css" rel="stylesheet" />
    <link href="~/css/common/warring.css" rel="stylesheet" />
    <link href="~/css/asset.css" rel="stylesheet" />
    @RenderSection("Links", required: false)
</head>
<body>
    <div class="wrapper container-fluid h-100">
        <div class="row h-100">
            <div class="left col-auto ">
                <div class="navbar">
                    <div class="logo-site">
                        <button class="qlts-logo m-button-clear m-background-image"></button>
                        <div class="qlts-text">QLTS.VN</div>
                        <button class="toggle-icon m-background-image"></button>
                    </div>
                    <div class="navbar-content">
                        <div class="navbar-item-box">
                            <a class="navbar-item" href="/#">
                                <div class="navbar-item-icon m-background-image navbar-item-computer"></div>
                                <div class="navbar-item-text">Tổng quan</div>
                            </a>
                        </div>
                        <div class="navbar-item-box has-submenu">
                            <div class="navbar-item">
                                <div class="navbar-item-icon m-background-image navbar-item-community"></div>
                                <div class="navbar-item-text">Tài sản</div>
                                <div class="navbar-item-direct">
                                    <i class="fi-sr-caret-right"></i>
                                </div>
                            </div>
                            <ul class="submenu">
                                <li><a href="/Asset/AssetIncreased">Ghi tăng</a></li>
                                <li><a href="/Asset/RefTransfer">Điều chuyển</a></li>
                                <li><a href="/Depreciation">Tính hao mòn</a></li>
                                <li><a href="#">Ghi giảm</a></li>
                            </ul>
                        </div>
                        <div class="navbar-item-box">
                            <a class="navbar-item" href="/#">
                                <div class="navbar-item-icon m-background-image navbar-item-pain-brush"></div>
                                <div class="navbar-item-text">Công cụ dụng cụ</div>
                            </a>
                        </div>
                        <div class="navbar-item-box has-submenu">
                            <div class="navbar-item" href="/#">
                                <div class="navbar-item-icon m-background-image navbar-item-common"></div>
                                <div class="navbar-item-text">Danh mục</div>
                                <div class="navbar-item-direct">
                                    <i class="fi-sr-caret-right"></i>
                                </div>
                            </div>
                            <ul class="submenu">
                                <li><a href="/dictionary/assetcategorypartial">Loại tài sản</a></li>
                                <li><a href="/dictionary/departments">Bộ phận sử dụng TS</a></li>
                            </ul>
                        </div>
                        <div class="navbar-item-box">
                            <a class="navbar-item" href="/#">
                                <div class="navbar-item-icon m-background-image navbar-item-search"></div>
                                <div class="navbar-item-text">Tra cứu</div>
                            </a>
                        </div>
                        <div class="navbar-item-box">
                            <a class="navbar-item" href="/#">
                                <div class="navbar-item-icon m-background-image navbar-item-setting"></div>
                                <div class="navbar-item-text">Hệ thống</div>
                            </a>
                        </div>
                        <div class="navbar-item-box">
                            <a class="navbar-item" href="/#">
                                <div class="navbar-item-icon m-background-image navbar-item-pie-chart"></div>
                                <div class="navbar-item-text">Báo cáo</div>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="right col">
                <div class="header">
                    <div class="header-left">
                        <div class="parent-title">@ViewData["ParentTitle"]</div>
                        <div class="m-seperator m-background-image"></div>
                        <div class="child-title">@ViewData["ChildTitle"]</div>
                    </div>
                    <div class="header-right">
                        <div class="department">Sở tài chính</div>
                        <div class="year">
                            @*<select size="1">
                                    <option>Năm 2021</option>
                                    <option>Năm 2020</option>
                                    <option>Năm 2019</option>
                                </select>*@
                            <div class="text-year">Năm</div>
                            <input class="form-control" type="number" value="2021" min="2000" max="2030" id="example-number-input">
                        </div>
                        <button class="help m-background-image m-button-clear"></button>
                        <button class="notification m-background-image m-button-clear"></button>
                        <div class="divider m-background-image m-button-clear"></div>
                        <button class="avatar m-background-image m-button-clear"></button>
                        <button class="arrow-up m-background-image m-button-clear"></button>
                    </div>
                </div>
                <div class="content">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>

    <div class="warring">
        <div class="warring-title">
            <div class="title-warring">Quản lý tài sản - QLTSv2</div>
            <button class="close" type="submit"></button>
        </div>
        <div class="warring-bode">
            <div class="warring-content">
                <div class="warring-icon"></div>
                <div class="warring-notify"></div>
            </div>
            <div class="warring-button">
                <button id="btn-no-warring">Hủy</button>
                <button id="btn-yes-warring">Xóa</button>
                <button id="btn-ok-warring">Đóng</button>
            </div>
        </div>
    </div>

    <div class="success">
        <div class="success_icon"></div>
        <div class="success_content">abc</div>
        <button class="success_button" type="submit"></button>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-ui-1.12.1.custom/jquery-ui.js"></script>

    <script src="~/lib/Inputmask-5.x/dist/bindings/inputmask.binding.js"></script>
    <script src="~/lib/Inputmask-5.x/dist/inputmask.js"></script>
    <script src="~/lib/Inputmask-5.x/dist/jquery.inputmask.js"></script>

    @*<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>*@
    <script src="~/js/site.js" asp-append-version="true"></script>
    @*Script dùng chung*@
    <script src="~/js/Common/data.js"></script>
    <script src="~/js/Base/common.js"></script>
    <script src="~/lib/moment/moment.js"></script>
    <script src="~/js/Base/enum.js"></script>
    @*Script validate, xử lý sự kiện của form*@
    <script src="~/js/Base/baseForm.js"></script>
    @*Script render dữ liệu vào bảng*@
    <script src="~/js/Base/Grid.js"></script>
    <script src="~/js/Base/BaseGrid.js"></script>
    <script src="~/js/navigation.js"></script>
    <script src="~/js/Form/test.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
