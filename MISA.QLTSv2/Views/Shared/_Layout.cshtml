<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MISA.QLTSv2</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/uicons-solid-rounded/css/uicons-solid-rounded.css" rel="stylesheet" />
    <link href="~/css/common/common.css" rel="stylesheet" />
    <link href="~/css/common/main.css" rel="stylesheet" />
    <link href="~/css/common/navbar.css" rel="stylesheet" />
    <link href="~/css/common/header.css" rel="stylesheet" />
    <link href="~/css/common/content.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/css/common/button.css" rel="stylesheet" />
    <link href="~/css/common/grid.css" rel="stylesheet" />
    <link href="~/css/common/icon.css" rel="stylesheet" />
    <link href="~/css/common/input.css" rel="stylesheet" />
    <link href="~/css/common/layout.css" rel="stylesheet" />
    <link href="~/css/common/toolbar.css" rel="stylesheet" />
</head>
<body>
    <div class="wrapper container-fluid h-100">
        <div class="row h-100">
            <div class="left col-auto ">
                <div class="navbar">
                    <div class="logo-site">
                        <div class="qlts-logo"></div>
                        <div class="qlts-text">QLTS.VN</div>
                        <button class="toggle-icon m-background-image"></button>
                    </div>
                    <div class="navbar-content">
                        <div class="navbar-item-box">
                            <a class="navbar-item" href="/#">
                                <div class="navbar-item-icon m-background-image navbar-item-computer"></div>
                                <div class="navbar-item-text">Tổng quan</div>
                            </a>
                        </div>
                        <div class="navbar-item-box has-submenu">
                            <div class="navbar-item">
                                <div class="navbar-item-icon m-background-image navbar-item-community"></div>
                                <div class="navbar-item-text">Tài sản</div>
                                <div class="navbar-item-direct">
                                    <i class="fi-sr-caret-right"></i>
                                </div>
                            </div>
                            <ul class="submenu">
                                <li><a href="#">Ghi tăng</a></li>
                                <li><a href="#">Điều chuyển</a></li>
                                <li><a href="#">Tính hao mòn</a></li>
                                <li><a href="#">Ghi giảm</a></li>
                            </ul>
                        </div>
                        <div class="navbar-item-box">
                            <a class="navbar-item" href="/#">
                                <div class="navbar-item-icon m-background-image navbar-item-pain-brush"></div>
                                <div class="navbar-item-text">Công cụ dụng cụ</div>
                            </a>
                        </div>
                        <div class="navbar-item-box has-submenu">
                            <div class="navbar-item" href="/#">
                                <div class="navbar-item-icon m-background-image navbar-item-common"></div>
                                <div class="navbar-item-text">Danh mục</div>
                                <div class="navbar-item-direct">
                                    <i class="fi-sr-caret-right"></i>
                                </div>
                            </div>
                            <ul class="submenu">
                                <li><a href="#">Loại tài sản</a></li>
                                <li><a href="#">Bộ phận sử dụng TS</a></li>
                            </ul>
                        </div>
                        <div class="navbar-item-box">
                            <a class="navbar-item" href="/#">
                                <div class="navbar-item-icon m-background-image navbar-item-search"></div>
                                <div class="navbar-item-text">Tra cứu</div>
                            </a>
                        </div>
                        <div class="navbar-item-box">
                            <a class="navbar-item" href="/#">
                                <div class="navbar-item-icon m-background-image navbar-item-setting"></div>
                                <div class="navbar-item-text">Hệ thống</div>
                            </a>
                        </div>
                        <div class="navbar-item-box">
                            <a class="navbar-item" href="/#">
                                <div class="navbar-item-icon m-background-image navbar-item-pie-chart"></div>
                                <div class="navbar-item-text">Báo cáo</div>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="right col">
                <div class="header">
                    <div class="header-left">
                        <div class="parent-title">Tài sản</div>
                        <div class="m-seperator m-background-image"></div>
                        <div class="child-title">Danh sách tài sản</div>
                    </div>
                    <div class="header-right">
                        <div class="department">Sở tài chính</div>
                        <div class="year">
                            <select size="1">
                                <option>Năm 2021</option>
                                <option>Năm 2020</option>
                                <option>Năm 2019</option>
                            </select>
                        </div>
                        <button class="help m-background-image m-button-clear"></button>
                        <button class="notification m-background-image m-button-clear"></button>
                        <div class="divider m-background-image m-button-clear"></div>
                        <button class="avatar m-background-image m-button-clear"></button>
                        <button class="arrow-up m-background-image m-button-clear"></button>
                    </div>
                </div>
                <div class="content">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
