
@{
    ViewData["Title"] = "LayoutView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="tool-bar">
    <div class="filter-bar">
        <input id="txtsearch" class="input-search icon-search" type="text" placeholder="Tìm Kiếm" />
    </div>
    <div class="tool-bar-right">
        <div>
            <button id="add-demo" class="button btn-add">Thêm</button>
        </div>
        <div>
            <button class="button btn-refresh">
                <div class="icon-refresh">

                </div>
            </button>
        </div>
        <div>
            <button class="button btn-remove">
                <div class="icon-remove ">
                </div>
            </button>
        </div>
    </div>
</div>
<div class="grid">
    <div class="grid-default">
        <table border="0" cellspacing="0" id="gridTest">
            <thead>
                <tr>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>
<div id="dialog_dictionary" class="dialog_dictionary" style="display:block">
    <div>
        <div>Mã loại tài sản (*)</div>
        <input class="input-text" fieldName="AssetTypeCode" required />
    </div>
    <div>
        <div>Tên loại tài sản (*)</div>
        <input class="input-text" fieldName="AssetTypeName" required />
    </div>
    <div>
        <div>Thuộc loại (*)</div>
        <input class="input-text" fieldName="AssetClassName" required />
    </div>
    <div>
        <div>Nhóm tài sản</div>
        <input class="input-text" fieldName="AssetGroupName" />
    </div>
    <div>
        <div>Tỷ lệ hao mòn</div>
        <input class="input-text" fieldName="WearPercent" />
    </div>
    <div>
        <div>Số năm sử dụng</div>
        <input class="input-text" fieldName="YearOfUse" />
    </div>
    <div>
        <div>Ghi chú</div>
        <textarea class="input-detail" fieldName="Note"></textarea>
    </div>
    <div class="dialog-footer">
        <div class="footer-left">
            <button class="button btn-cancel" id="btn-cancel">Hủy</button>
        </div>
        <div class="footer-right">
            <button class="button btn-add" id="btn-save">Lưu</button>
        </div>
    </div>
</div>
<div class="menu">
    <p>Option 1</p>
    <p>Option 2</p>
</div>
<div class="footer-layout">
    <div>
        Tổng số tài sản: 1.015
    </div>
    <div class="footer-right">
        Tổng nguyên giá: 1.546.354.789
    </div>
</div>





